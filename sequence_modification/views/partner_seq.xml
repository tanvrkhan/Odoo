<odoo>
    <data>
        <record id="partner_sequence_tree" model="ir.ui.view">
            <field name="name">Partner Sequence Tree</field>
            <field name="model">partner.sequence</field>
            <field name="arch" type="xml">
                <tree string="Partner Sequence">
                    <field name="name"/>
                    <field name="next_number"/>
                </tree>
            </field>
        </record>
        <record id="partner_sequence_action_id" model="ir.actions.act_window">
            <field name="name">Partner Sequence</field>
            <field name="res_model">partner.sequence</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="partner_seq_id" name="Partner Sequence" parent="contacts.menu_contacts"
                  action="partner_sequence_action_id" sequence="10"/>
    </data>
</odoo>