<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- International-->
        <record id="product_product_freight_charges" model="product.product">
            <field name="name">Freight Charges - CBM</field>
            <field name="default_code">freight_charge</field>
            <field name="type">service</field>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
        </record>
        <record id="model_stock_valuation_button_update_valuation" model="ir.actions.server">
            <field name="name">Update stock valuation</field>
            <field name="model_id" ref="stock_account.model_stock_valuation_layer"/>
            <field name="binding_model_id" ref="stock_account.model_stock_valuation_layer"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.update_stock_value_to_match_initial_if_cancelled_out()</field>
        </record>
        <record id="model_stock_valuation_action_unset_value" model="ir.actions.server">
            <field name="name">Remove stock valuation</field>
            <field name="model_id" ref="stock_account.model_stock_valuation_layer"/>
            <field name="binding_model_id" ref="stock_account.model_stock_valuation_layer"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.remove_from_valuation()</field>
        </record>
        <record id="model_stock_picking_action_set_draft" model="ir.actions.server">
            <field name="name">Set to draft</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list,form</field>
            <field name="state">code</field>
            <field name="code">records.set_stock_move_to_draft()</field>
        </record>
        <record id="model_stock_picking_action_set_quant_0" model="ir.actions.server">
            <field name="name">Set All Quant to 0</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.stock_quant_zero()</field>
        </record>
        <record id="model_stock_picking_action_sync_quant" model="ir.actions.server">
            <field name="name">Sync Quant</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.sync_stock_quant()</field>
        </record>
        <record id="model_stock_picking_action_delete_adjustments" model="ir.actions.server">
            <field name="name">Delete stock adjustments</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.delete_all_inventory_adjustments()</field>
        </record>
        <record id="model_stock_picking_action_delete_internal_transfers" model="ir.actions.server">
            <field name="name">Delete stock internal transfers</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.delete_all_internal_transfers()</field>
        </record>
        <record id="model_stock_move_action_fix_unmatching_locations" model="ir.actions.server">
            <field name="name">Fix unmatching locations</field>
            <field name="model_id" ref="stock.model_stock_move_line"/>
            <field name="binding_model_id" ref="stock.model_stock_move_line"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">records.fix_incoming_outgoing_locations()</field>
        </record>

    </data>
</odoo>