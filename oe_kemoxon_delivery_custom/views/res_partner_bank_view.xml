<odoo>
    <data>
        <record id="view_partner_bank_form_inherit_account" model="ir.ui.view">
            <field name="name">res.partner.bank.form</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="account.view_partner_bank_form_inherit_account"/>
            <field name="arch" type="xml">
                <field name="currency_id" position="after">
                    <field name="inter_mediatory_bank_id"/>
                    <field name="inter_mediatory_swift"/>
                </field>
            </field>
        </record>
         <record id="inheritMailTemplateForm" model="ir.ui.view">
            <field name="name">inherit_mail_template_form</field>
            <field name="model">mail.template</field>
            <field name="inherit_id" ref="mail.email_template_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='email_to']" position="after">
                    <field name="not_send_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>